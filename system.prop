# gfx setup (gles 3.0)
ro.opengles.version=196608
debug.hwui.render_dirty_regions=false
ro.nx.mma=1

# middleware heaps configuration
ro.nx.heap.video_secure=86m
ro.nx.heap.main=112m
ro.nx.heap.drv_managed=0m
ro.nx.heap.grow=8m
ro.nx.heap.shrink=2m
ro.nx.heap.gfx=64m

# hwc optimizations
ro.nx.capable.cb=1
ro.nx.capable.si=1
ro.sf.lcd_density=320

ro.rfkilldisabled=1
ro.nx.eth.irq_mode_mask=3:2

# google assigned client id
ro.com.google.clientidbase=android-avko-tv

ro.nrdp.modelgroup=AVKO

